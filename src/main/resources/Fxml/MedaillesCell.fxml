<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="40.0" prefWidth="550.0" styleClass="cell_container" stylesheets="@../Styles/Cell.css"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fx:id="flag_img" fitHeight="30.0" fitWidth="30.0" layoutX="14.0" layoutY="8.0" pickOnBounds="true"
               preserveRatio="true" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="14.0"
               AnchorPane.topAnchor="5.0"/>
    <HBox layoutX="50.0" prefHeight="40.0" prefWidth="450.0" AnchorPane.leftAnchor="80.0">
        <Label fx:id="name_lbl" text="Name"/>
        <Label fx:id="golden_lbl" text="0">
            <graphic>
                <ImageView fx:id="golden_imgView" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </graphic>
        </Label>
        <Label fx:id="silver_lbl" text="0">
            <graphic>
                <ImageView fx:id="silver_imgView" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </graphic>
        </Label>
        <Label fx:id="bronze_lbl" text="0">
            <graphic>
                <ImageView fx:id="bronze_imgView" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </graphic>
        </Label>
        <Label fx:id="all_medals_lbl" text="0">
            <graphic>
                <ImageView fx:id="all_medals_imgView" fitHeight="10.0" fitWidth="10.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </graphic>
        </Label>
    </HBox>
</AnchorPane>
